{
	"name": "frontend-boilerplate",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky install",
		"commit": "cz"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		]
	},
	"dependencies": {
		"eslint-release": "^3.2.0",
		"next": "12.0.10",
		"next-images": "^1.8.4",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-icons": "^4.3.1",
		"styled-components": "^5.3.5"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "./.cz-config.js"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/parse": "^16.0.0",
		"@types/node": "17.0.15",
		"@types/react": "17.0.39",
		"@types/styled-components": "^5.1.21",
		"babel-plugin-styled-components": "^2.0.2",
		"commitizen": "^4.2.4",
		"commitlint-config-cz": "^0.13.3",
		"cz-customizable": "^6.3.0",
		"eslint": "8.8.0",
		"eslint-config-infojr-ts": "^1.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.3",
		"typescript": "4.5.5"
	}
}
